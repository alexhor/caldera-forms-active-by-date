#: caldera-forms-active-by-date.php:34
#, php-format
msgid "Form should only be active from %s to %s"
msgstr "Das Formular soll nur von %s bis %s aktiv sein"

#: caldera-forms-active-by-date.php:39
msgid "Message to show before active period"
msgstr "Nachricht die angezeigt werden soll, bevor das Formular aktiv ist"

#: caldera-forms-active-by-date.php:43
msgid "Use %date% to insert the start date."
msgstr "Benutze %date% um das Startdatum einzufügen."

#: caldera-forms-active-by-date.php:48
msgid "Message to show after active period"
msgstr "Nachricht die angezeigt werden soll, nachdem das Formular nicht mehr aktiv ist"

#: caldera-forms-active-by-date.php:52
msgid "Use %date% to insert the end date."
msgstr "Benutze %date% um das Enddatum einzufügen."

#: caldera-forms-active-by-date.php:90
msgid "This form isn't active until %date%"
msgstr "Dieses Formular ist nicht aktiv bis %date%"

#: caldera-forms-active-by-date.php:96
msgid "This form is disabled since %date%"
msgstr "Dieses Formular ist bis %date% deaktiviert"
